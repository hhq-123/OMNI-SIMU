page {
  height: 100%;
}
.container-fill{
  height: 100%;
  overflow: hidden;
 }

 .scroll-fullpage{
  height: 100%;
  transition: all 0.3s;
 }

 .section{
  height: 100%;
 }

 .logosize{
  display: flex;
  height: 100%; 
  justify-content: center;
  align-items: center;
 }

 .logosize image { 
   width:500rpx;
   height:500rpx;
   }

 .logn_btn{
  display:flex;  
  justify-content: center;  
  width: 400rpx;
  
  border-radius: 75rpx;

  left: 130px;
  top: 627px;
  width: 149px;
  height: 48px;
  border-radius: 30px;
  color: rgba(34, 137, 234, 100);
  font-size: 20px;
  text-align: center;
  font-family: Microsoft Yahei;
  border: 1px solid rgba(255, 255, 255, 100);
}



 .active .section-maintitle,
 .active .section-subtitle{
  animation: mymove 0.8s;
 }
 @keyframes mymove{
  from {
  transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
  }
  to {
  transform: translateY(0) scale(1) rotateY(0);
  }
 }